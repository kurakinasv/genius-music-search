{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@app/*": [
                "src/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@config/*": [
                "src/config/*"
            ],
            "@icons/*": [
                "src/icons/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@store/*": [
                "src/store/*"
            ],
            "@type/*": [
                "src/types/*"
            ]
        }
    }
}